config VIDEO_WOKOO_CSI
	tristate "CodeFair WOKOO CMOS Sensor Interface Support"
	depends on VIDEO_V4L2 && COMMON_CLK && VIDEO_V4L2_SUBDEV_API && HAS_DMA
	depends on ARCH_WOKOO
	select VIDEOBUF2_DMA_CONTIG
	select V4L2_FWNODE
	help
	  This is a V4L2 driver for the CodeFair WOKOO CSI
	  To compile this driver as a module, choose M here: the module
	  will be called wokoo_csi.